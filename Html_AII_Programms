    <!-- div tag basically used as a container in which one can add any type of elements -->
    <h1>
      div tag is block level element used as a container to wrap html elements
    </h1>
    <div>
      <!-- adding inline level elements -->
      <a href="">link</a>
      <button>login</button>
      <span>span</span>
      <img src="" alt="" />
      <input type="text" />
      <strong>data</strong>
      <b>bold</b>
      <u>underline</u>
      <!-- adding block level elements -->
      <p>para</p>
      <h1>head</h1>
      <section>section</section>
      <ul>
        <li>data</li>
        <li>data</li>
        <li>data</li>
        <li>data</li>
      </ul>
      <div>div</div>
      <pre>pre</pre>
      <article>article</article>
    </div>
