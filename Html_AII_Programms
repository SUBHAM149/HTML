    <h1 style="background-color: rgb(0, 60, 255);">CLASS-14</h1>
    <form action="">
      <!-- use 'value' attribute to give the color by using hexadecimal values -->
      <input type="color" name="color" id="" value="#008000" />
      <!-- multiple is an attribute used in input type 'file' that provides user to select multiple files -->
      <input type="file" multiple name="files" />
      <br />
      <!-- Output is a tag used for showing result -->
      <input
        type="text"
        placeholder="Enter a number n1"
        name="n1"
        value="10"
      />+
      <input
        type="text"
        placeholder="Enter a number n2"
        name="n2"
        value="20"
      />=
      <output>Result :30</output>
      <!-- 'low' and 'high' attributes defines the range of a meter tag. Optimum means 'actual value'  -->
      <meter
        value="90"
        min="5"
        max="100"
        low="30"
        high="70"
        optimum="100"
      ></meter><br>
      <progress value="80" max="100"></progress>
    </form>
    <h1 style="background-color: rgb(129, 139, 114);">CLASS-15</h1>
    <header>
        this header tag is used for main heading of the webpage
        <h1>Heading</h1>
    </header>
    <nav>
        navBar
    </nav>
    <section>
        section tag used to include other block and inline level elements
    </section>

    <!-- the difference between p and article tag -->
     <p>
        this is paragraph  
        <!-- do not take block level elements inside p tag. If took, browser will remove it from p tag -->
        <div>I m div tag</div>
     </p>
<!-- article is a independent tag that can any block and inline level elements -->
     <article>
        this is article tag
        <div>I m div tag</div>
     </article>

     <!-- main tag is used to create the main content -->
      <main>
        main content
      </main>

      <!-- aside tag is used to give the content in any side of the page. Either left or right side of the page -->
       <aside>
        this is aside tag
       </aside>

       <!-- figure and figcaption tags -->
        <!-- previous use of image tag -->
        <div><img src="" alt=""></div>
        <!-- after -->
         <figure>
            <img src="../Assets/Images/krishna photo.jpeg" alt=" Hii I am Krishna" title="Dhoni image india">
            <figcaption>Krishna from universe</figcaption>

         </figure>

         <details>
            <summary>HTML</summary>
            <p>Hyper Text Markup Language</p>
         </details>

